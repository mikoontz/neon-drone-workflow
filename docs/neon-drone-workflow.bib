  author = {Kellner, James R. and Armston, John and Birrer, Markus and Cushman, K. C. and Duncanson, Laura and Eck, Christoph and Falleger, Christoph and Imbach, Benedikt and Kr\'al, Kamil and Kr\r{u}\v{c}ek, Martin and Trochta, Jan and Vr\v{s}ka, Tom\'a\v{s} and Zgraggen, Carlo},
