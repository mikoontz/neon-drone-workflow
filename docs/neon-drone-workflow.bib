@article{piermattei2019,
  title = {Terrestrial Structure from Motion Photogrammetry for Deriving Forest Inventory Data},
  author = {Piermattei, Livia and Karel, Wilfried and Wang, Di and Wieser, Martin and Mokro\v{s}, Martin and Surov\'y, Peter and Kore\v{n}, Milan and Toma\v{s}t\'ik, Juli\'an and Pfeifer, Norbert and Hollaus, Markus},
