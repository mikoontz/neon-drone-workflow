  shorttitle = {Beyond Ecosystem Modeling},
  author = {Fer, Istem and Gardella, Anthony K. and Shiklomanov, Alexey N. and Campbell, Eleanor E. and Cowdery, Elizabeth M. and Kauwe, Martin G. De and Desai, Ankur and Duveneck, Matthew J. and Fisher, Joshua B. and Haynes, Katherine D. and Hoffman, Forrest M. and Johnston, Miriam R. and Kooper, Rob and LeBauer, David S. and Mantooth, Joshua and Parton, William J. and Poulter, Benjamin and Quaife, Tristan and Raiho, Ann and Schaefer, Kevin and Serbin, Shawn P. and Simkins, James and Wilcox, Kevin R. and Viskari, Toni and Dietze, Michael C.},
  date = {2021},
  journaltitle = {Global Change Biology},
