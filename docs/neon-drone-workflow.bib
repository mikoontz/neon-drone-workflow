@article{togeirodealckmin2020,
  title = {Retrieval of {{Hyperspectral Information}} from {{Multispectral Data}} for {{Perennial Ryegrass Biomass Estimation}}},
  author = {Togeiro de Alckmin, Gustavo and Kooistra, Lammert and Rawnsley, Richard and de Bruin, Sytze and Lucieer, Arko},
  date = {2020-01},
