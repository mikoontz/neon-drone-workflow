@article{assmann2019,
  title = {Vegetation Monitoring Using Multispectral Sensors \textemdash{} Best Practices and Lessons Learned from High Latitudes},
  author = {Assmann, Jakob J. and Kerby, Jeffrey T. and Cunliffe, Andrew M. and Myers-Smith, Isla H.},
