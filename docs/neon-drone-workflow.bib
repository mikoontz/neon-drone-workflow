  author = {Camps-Valls, Gustau and Campos-Taberner, Manuel and Moreno-Mart\'inez, \'Alvaro and Walther, Sophia and Duveiller, Gr\'egory and Cescatti, Alessandro and Mahecha, Miguel D. and Mu\~noz-Mar\'i, Jordi and Garc\'ia-Haro, Francisco Javier and Guanter, Luis and Jung, Martin and Gamon, John A. and Reichstein, Markus and Running, Steven W.},
