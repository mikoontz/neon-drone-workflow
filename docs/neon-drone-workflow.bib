@article{andelman2004,
  title = {Networks by {{Design}}: {{A Revolution}} in {{Ecology}}},
  shorttitle = {Networks by {{Design}}},
  author = {Andelman, Sandy J. and Willig, Michael R.},
  date = {2004-09-10},
  journaltitle = {Science},
  volume = {305},
  pages = {1565--1567},
  publisher = {{American Association for the Advancement of Science}},
  issn = {0036-8075, 1095-9203},
