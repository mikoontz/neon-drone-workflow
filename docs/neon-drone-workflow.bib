@article{parsons2011,
  title = {Linking {{3D}} Spatial Models of Fuels and Fire: {{Effects}} of Spatial Heterogeneity on Fire Behavior},
  shorttitle = {Linking {{3D}} Spatial Models of Fuels and Fire},
  author = {Parsons, Russell A. and Mell, William E. and McCauley, Peter},
  date = {2011-02},
  journaltitle = {Ecological Modelling},
  shortjournal = {Ecological Modelling},
  volume = {222},
